// Minimal async test - correct Aria syntax with async keyword
// Note: async functions return i8* (coroutine handle), not their declared type
{
async func:test_async = int8() {
    int8:value = 42;
    await value;  // Test await expression
    pass(value);
};

func:main = void() {
    test_async();  // Just call it, don't capture
    print("Async test completed");
};
}
