{
// EXACT spec example from Aria v0.0.6 Section 8.3
// Pattern matching with fallthrough labels

int8:c = 9;
int8:res = 0;

pick(c) {
   (<9) {
       fall(fail);
   },
   (>9) {
       fall(fail);
   },
   (9) {
       fall(success);
   },
   (*) {
       fall(err);
   },
   fail:(!) {
       res = 1;
       fall(done);
   },
   success:(!) {
       res = 2;
       fall(done);
   },
   err:(!) {
       res = 3;
       fall(done);
   },
   done:(!) {
       res = res + 10;
   }
}
}
