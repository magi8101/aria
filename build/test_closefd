; ModuleID = 'aria_module'
source_filename = "aria_module"

%result_int8 = type { i8, i8 }

declare void @puts(ptr)

declare void @print(ptr)

define internal void @__aria_module_init() {
entry:
  ret void
}

define internal %result_int8 @test() {
entry:
  %status = alloca i32, align 4
  %fd = alloca i32, align 4
  %syscall_result = call i64 asm sideeffect "syscall", "={rax},{rax},{rdi},{rsi},{rdx}"(i64 2, i64 0, i64 0, i64 0)
  %syscall_cast = trunc i64 %syscall_result to i32
  store i32 %syscall_cast, ptr %fd, align 4
  %0 = load i32, ptr %fd, align 4
  %1 = sext i32 %0 to i64
  %syscall_result1 = call i64 asm sideeffect "syscall", "={rax},{rax},{rdi}"(i64 3, i64 %1)
  %syscall_cast2 = trunc i64 %syscall_result1 to i32
  store i32 %syscall_cast2, ptr %status, align 4
  %result = alloca %result_int8, align 8
  %err_ptr = getelementptr inbounds %result_int8, ptr %result, i32 0, i32 0
  store i8 0, ptr %err_ptr, align 1
  %val_ptr = getelementptr inbounds %result_int8, ptr %result, i32 0, i32 1
  store i8 0, ptr %val_ptr, align 1
  %result_val = load %result_int8, ptr %result, align 1
  ret %result_int8 %result_val
}

define i64 @main() {
entry:
  call void @__aria_module_init()
  ret i64 0
}
