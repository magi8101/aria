// Test closures - capturing variables from outer scope
int8:closureTest = 2;

// Function that captures closureTest from global scope
func:test = int8(int8:a, int8:b) {
    return a * b * closureTest;  // Should capture global closureTest
};

func:main = int8() {
    return test(3, 4);  // Should be 3 * 4 * 2 = 24
};
