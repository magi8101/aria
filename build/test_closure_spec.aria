// Test closures - spec compliant with result objects
int8:closureTest = 2;

// All functions must return result objects per spec
func:test_closure = *int8(int8:a, int8:b) {
    int8:product = a * b * closureTest;  // Captures global closureTest
    return product;
};

func:main = *int8() {
    return test_closure(3, 4);
};
