; ModuleID = 'aria_module'
source_filename = "aria_module"

%result_int8 = type { i8, i8 }
%result_uint8 = type { i8, i8 }

@0 = private unnamed_addr constant [41 x i8] c"========================================\00", align 1
@1 = private unnamed_addr constant [38 x i8] c"  Comprehensive Bitwise Operator Test\00", align 1
@2 = private unnamed_addr constant [41 x i8] c"========================================\00", align 1
@3 = private unnamed_addr constant [43 x i8] c"\E2\9C\93 All 5 bitwise compound operators: PASS\00", align 1
@4 = private unnamed_addr constant [37 x i8] c"\E2\9C\97 Bitwise compound operators: FAIL\00", align 1
@5 = private unnamed_addr constant [40 x i8] c"\E2\9C\93 REX prefix calculation: PASS (0x48)\00", align 1
@6 = private unnamed_addr constant [33 x i8] c"\E2\9C\97 REX prefix calculation: FAIL\00", align 1
@7 = private unnamed_addr constant [41 x i8] c"\E2\9C\93 ModR/M byte calculation: PASS (0xF8)\00", align 1
@8 = private unnamed_addr constant [34 x i8] c"\E2\9C\97 ModR/M byte calculation: FAIL\00", align 1
@9 = private unnamed_addr constant [41 x i8] c"========================================\00", align 1
@10 = private unnamed_addr constant [22 x i8] c"  All tests complete!\00", align 1
@11 = private unnamed_addr constant [41 x i8] c"========================================\00", align 1

declare void @puts(ptr)

declare void @print(ptr)

define internal void @__aria_module_init() {
entry:
  ret void
}

define internal %result_int8 @test_all_operators() {
entry:
  %success = alloca i8, align 1
  %e = alloca i8, align 1
  %d = alloca i8, align 1
  %c = alloca i8, align 1
  %b = alloca i8, align 1
  %a = alloca i8, align 1
  store i8 -1, ptr %a, align 1
  %0 = load i8, ptr %a, align 1
  %1 = sext i8 %0 to i64
  store i8 64, ptr %b, align 1
  %2 = load i8, ptr %b, align 1
  %3 = sext i8 %2 to i64
  store i8 -1, ptr %c, align 1
  %4 = load i8, ptr %c, align 1
  %5 = sext i8 %4 to i64
  store i8 1, ptr %d, align 1
  %6 = load i8, ptr %d, align 1
  %7 = sext i8 %6 to i64
  store i8 -128, ptr %e, align 1
  %8 = load i8, ptr %e, align 1
  %9 = sext i8 %8 to i64
  store i8 1, ptr %success, align 1
  %10 = load i8, ptr %a, align 1
  %11 = sext i8 %10 to i64
  %netmp = icmp ne i64 %11, 15
  br i1 %netmp, label %then, label %ifcont

then:                                             ; preds = %entry
  store i8 0, ptr %success, align 1
  br label %ifcont

ifcont:                                           ; preds = %then, %entry
  %12 = load i8, ptr %b, align 1
  %13 = sext i8 %12 to i64
  %netmp1 = icmp ne i64 %13, 72
  br i1 %netmp1, label %then2, label %ifcont3

then2:                                            ; preds = %ifcont
  store i8 0, ptr %success, align 1
  br label %ifcont3

ifcont3:                                          ; preds = %then2, %ifcont
  %14 = load i8, ptr %c, align 1
  %15 = sext i8 %14 to i64
  %netmp4 = icmp ne i64 %15, 170
  br i1 %netmp4, label %then5, label %ifcont6

then5:                                            ; preds = %ifcont3
  store i8 0, ptr %success, align 1
  br label %ifcont6

ifcont6:                                          ; preds = %then5, %ifcont3
  %16 = load i8, ptr %d, align 1
  %17 = sext i8 %16 to i64
  %netmp7 = icmp ne i64 %17, 16
  br i1 %netmp7, label %then8, label %ifcont9

then8:                                            ; preds = %ifcont6
  store i8 0, ptr %success, align 1
  br label %ifcont9

ifcont9:                                          ; preds = %then8, %ifcont6
  %18 = load i8, ptr %e, align 1
  %19 = sext i8 %18 to i64
  %netmp10 = icmp ne i64 %19, 32
  br i1 %netmp10, label %then11, label %ifcont12

then11:                                           ; preds = %ifcont9
  store i8 0, ptr %success, align 1
  br label %ifcont12

ifcont12:                                         ; preds = %then11, %ifcont9
  %20 = load i8, ptr %success, align 1
  %auto_wrap_result = alloca %result_int8, align 8
  %err_ptr = getelementptr inbounds %result_int8, ptr %auto_wrap_result, i32 0, i32 0
  store i8 0, ptr %err_ptr, align 1
  %val_ptr = getelementptr inbounds %result_int8, ptr %auto_wrap_result, i32 0, i32 1
  store i8 %20, ptr %val_ptr, align 1
  %result_val = load %result_int8, ptr %auto_wrap_result, align 1
  ret %result_int8 %result_val
}

define internal %result_uint8 @test_rex_calculation() {
entry:
  %rex = alloca i8, align 1
  store i8 64, ptr %rex, align 1
  %0 = load i8, ptr %rex, align 1
  %1 = sext i8 %0 to i64
  %2 = load i8, ptr %rex, align 1
  %auto_wrap_result = alloca %result_uint8, align 8
  %err_ptr = getelementptr inbounds %result_uint8, ptr %auto_wrap_result, i32 0, i32 0
  store i8 0, ptr %err_ptr, align 1
  %val_ptr = getelementptr inbounds %result_uint8, ptr %auto_wrap_result, i32 0, i32 1
  store i8 %2, ptr %val_ptr, align 1
  %result_val = load %result_uint8, ptr %auto_wrap_result, align 1
  ret %result_uint8 %result_val
}

define internal %result_uint8 @test_modrm_calculation() {
entry:
  %modrm = alloca i8, align 1
  store i8 0, ptr %modrm, align 1
  %0 = load i8, ptr %modrm, align 1
  %1 = sext i8 %0 to i64
  %2 = load i8, ptr %modrm, align 1
  %3 = sext i8 %2 to i64
  %4 = load i8, ptr %modrm, align 1
  %5 = load i8, ptr %modrm, align 1
  %auto_wrap_result = alloca %result_uint8, align 8
  %err_ptr = getelementptr inbounds %result_uint8, ptr %auto_wrap_result, i32 0, i32 0
  store i8 0, ptr %err_ptr, align 1
  %val_ptr = getelementptr inbounds %result_uint8, ptr %auto_wrap_result, i32 0, i32 1
  store i8 %5, ptr %val_ptr, align 1
  %result_val = load %result_uint8, ptr %auto_wrap_result, align 1
  ret %result_uint8 %result_val
}

define internal %result_int8 @__user_main() {
entry:
  %modrm = alloca i8, align 1
  %rex = alloca i8, align 1
  %test1 = alloca i8, align 1
  call void @puts(ptr @0)
  call void @puts(ptr @1)
  call void @puts(ptr @2)
  %calltmp = call %result_int8 @test_all_operators()
  store %result_int8 %calltmp, ptr %test1, align 1
  %0 = load i8, ptr %test1, align 1
  %1 = sext i8 %0 to i64
  %eqtmp = icmp eq i64 %1, 1
  br i1 %eqtmp, label %then, label %else

then:                                             ; preds = %entry
  call void @puts(ptr @3)
  br label %ifcont

else:                                             ; preds = %entry
  call void @puts(ptr @4)
  br label %ifcont

ifcont:                                           ; preds = %else, %then
  %calltmp1 = call %result_uint8 @test_rex_calculation()
  store %result_uint8 %calltmp1, ptr %rex, align 1
  %2 = load i8, ptr %rex, align 1
  %3 = sext i8 %2 to i64
  %eqtmp2 = icmp eq i64 %3, 72
  br i1 %eqtmp2, label %then3, label %else4

then3:                                            ; preds = %ifcont
  call void @puts(ptr @5)
  br label %ifcont5

else4:                                            ; preds = %ifcont
  call void @puts(ptr @6)
  br label %ifcont5

ifcont5:                                          ; preds = %else4, %then3
  %calltmp6 = call %result_uint8 @test_modrm_calculation()
  store %result_uint8 %calltmp6, ptr %modrm, align 1
  %4 = load i8, ptr %modrm, align 1
  %5 = sext i8 %4 to i64
  %eqtmp7 = icmp eq i64 %5, 248
  br i1 %eqtmp7, label %then8, label %else9

then8:                                            ; preds = %ifcont5
  call void @puts(ptr @7)
  br label %ifcont10

else9:                                            ; preds = %ifcont5
  call void @puts(ptr @8)
  br label %ifcont10

ifcont10:                                         ; preds = %else9, %then8
  call void @puts(ptr @9)
  call void @puts(ptr @10)
  call void @puts(ptr @11)
  %auto_wrap_result = alloca %result_int8, align 8
  %err_ptr = getelementptr inbounds %result_int8, ptr %auto_wrap_result, i32 0, i32 0
  store i8 0, ptr %err_ptr, align 1
  %val_ptr = getelementptr inbounds %result_int8, ptr %auto_wrap_result, i32 0, i32 1
  store i8 0, ptr %val_ptr, align 1
  %result_val = load %result_int8, ptr %auto_wrap_result, align 1
  ret %result_int8 %result_val
}

define i64 @main() {
entry:
  call void @__aria_module_init()
  %0 = call %result_int8 @__user_main()
  ret i64 0
}
