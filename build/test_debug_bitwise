; ModuleID = 'aria_module'
source_filename = "aria_module"

%result_int8 = type { i8, i8 }

@0 = private unnamed_addr constant [33 x i8] c"Testing individual operations...\00", align 1
@1 = private unnamed_addr constant [17 x i8] c"a after &= 0x0F:\00", align 1
@2 = private unnamed_addr constant [17 x i8] c"b after |= 0x08:\00", align 1
@3 = private unnamed_addr constant [19 x i8] c"rex after |= 0x08:\00", align 1
@4 = private unnamed_addr constant [18 x i8] c"Comparison works!\00", align 1
@5 = private unnamed_addr constant [19 x i8] c"Comparison broken!\00", align 1

declare void @puts(ptr)

declare void @print(ptr)

define internal void @__aria_module_init() {
entry:
  ret void
}

define internal %result_int8 @__user_main() {
entry:
  %test = alloca i8, align 1
  %rex = alloca i8, align 1
  %b = alloca i8, align 1
  %a = alloca i8, align 1
  call void @puts(ptr @0)
  store i8 -1, ptr %a, align 1
  %0 = load i8, ptr %a, align 1
  %1 = sext i8 %0 to i64
  call void @puts(ptr @1)
  store i8 64, ptr %b, align 1
  %2 = load i8, ptr %b, align 1
  %3 = sext i8 %2 to i64
  call void @puts(ptr @2)
  store i8 64, ptr %rex, align 1
  %4 = load i8, ptr %rex, align 1
  %5 = sext i8 %4 to i64
  call void @puts(ptr @3)
  store i8 72, ptr %test, align 1
  %6 = load i8, ptr %test, align 1
  %7 = sext i8 %6 to i64
  %eqtmp = icmp eq i64 %7, 72
  br i1 %eqtmp, label %then, label %else

then:                                             ; preds = %entry
  call void @puts(ptr @4)
  br label %ifcont

else:                                             ; preds = %entry
  call void @puts(ptr @5)
  br label %ifcont

ifcont:                                           ; preds = %else, %then
  %auto_wrap_result = alloca %result_int8, align 8
  %err_ptr = getelementptr inbounds %result_int8, ptr %auto_wrap_result, i32 0, i32 0
  store i8 0, ptr %err_ptr, align 1
  %val_ptr = getelementptr inbounds %result_int8, ptr %auto_wrap_result, i32 0, i32 1
  store i8 0, ptr %val_ptr, align 1
  %result_val = load %result_int8, ptr %auto_wrap_result, align 1
  ret %result_int8 %result_val
}

define i64 @main() {
entry:
  call void @__aria_module_init()
  %0 = call %result_int8 @__user_main()
  ret i64 0
}
