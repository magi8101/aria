// Test fail() and pass() syntactic sugar
func:divide = result<int64>(int64:a, int64:b) {
    if (b == 0) return fail(1);  // Error: division by zero
    return pass(a / b);           // Success with quotient
};

func:main = int64() {
    // Test success case
    result<int64>:r1 = divide(10, 2);
    
    // Test error case  
    result<int64>:r2 = divide(10, 0);
    
    return 0;
};
