// Test improved fail() and pass() - no return needed!
func:divide = int64(int64:a, int64:b) {
    if (b == 0) {
        fail(1);
    }
    pass(a / b);
};

func:check_range = int64(int64:x) {
    if (x < 0) {
        fail(2);
    }
    if (x > 100) {
        fail(3);
    }
    pass(x);
};

func:main = int64() {
    int64:r1 = divide(10, 2);
    int64:r2 = divide(10, 0);
    int64:r3 = check_range(50);
    int64:r4 = check_range(-5);
    return 0;
};
