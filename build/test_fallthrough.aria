// Test labeled pick with fallthrough using fall(label)
func:process_number = int8(int8:x) {
    pick(x) {
        positive=>(>0) {
            fall(done);
        },
        zero=>(0) {
            fall(done);
        },
        negative=>(<0) {
            fall(done);
        },
        done=>(!) {
            // Unreachable by pattern - only via fall()
            return x;
        }
    }
};

func:main = int8() {
    return process_number(5);
};
