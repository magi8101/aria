// test_func_signature.aria
// Test function pointer with signature

func:add = int64(int64:a, int64:b) {
    pass(a + b);
};

func:main = int8() {
    // Test 1: Assign function to func variable with signature
    func<int64(int64, int64)>:my_add = add;
    
    // Test 2: Call through func variable
    result:r = my_add(10, 20);
    
    // Test 3: Check result
    if (r.err == 0) {
        print("Function signature test passed!");
    } fail(1);
    
    *0;
};
