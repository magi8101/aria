; ModuleID = 'aria_module'
source_filename = "aria_module"

%result_int8 = type { i8, i8 }

@0 = private unnamed_addr constant [17 x i8] c"Testing syscalls\00", align 1
@1 = private unnamed_addr constant [12 x i8] c"File opened\00", align 1
@2 = private unnamed_addr constant [5 x i8] c"Done\00", align 1

declare void @puts(ptr)

declare void @print(ptr)

define internal void @__aria_module_init() {
entry:
  ret void
}

define internal %result_int8 @__user_main() {
entry:
  %status = alloca i32, align 4
  %fd = alloca i32, align 4
  %filename = alloca [16 x i8], align 1
  call void @puts(ptr @0)
  %filename_ptr = load ptr, ptr %filename, align 8
  %elem_ptr = getelementptr i8, ptr %filename_ptr, i64 0
  store i8 116, ptr %elem_ptr, align 1
  %filename_ptr1 = load ptr, ptr %filename, align 8
  %elem_ptr2 = getelementptr i8, ptr %filename_ptr1, i64 1
  store i8 101, ptr %elem_ptr2, align 1
  %filename_ptr3 = load ptr, ptr %filename, align 8
  %elem_ptr4 = getelementptr i8, ptr %filename_ptr3, i64 2
  store i8 115, ptr %elem_ptr4, align 1
  %filename_ptr5 = load ptr, ptr %filename, align 8
  %elem_ptr6 = getelementptr i8, ptr %filename_ptr5, i64 3
  store i8 116, ptr %elem_ptr6, align 1
  %filename_ptr7 = load ptr, ptr %filename, align 8
  %elem_ptr8 = getelementptr i8, ptr %filename_ptr7, i64 4
  store i8 46, ptr %elem_ptr8, align 1
  %filename_ptr9 = load ptr, ptr %filename, align 8
  %elem_ptr10 = getelementptr i8, ptr %filename_ptr9, i64 5
  store i8 116, ptr %elem_ptr10, align 1
  %filename_ptr11 = load ptr, ptr %filename, align 8
  %elem_ptr12 = getelementptr i8, ptr %filename_ptr11, i64 6
  store i8 120, ptr %elem_ptr12, align 1
  %filename_ptr13 = load ptr, ptr %filename, align 8
  %elem_ptr14 = getelementptr i8, ptr %filename_ptr13, i64 7
  store i8 116, ptr %elem_ptr14, align 1
  %filename_ptr15 = load ptr, ptr %filename, align 8
  %elem_ptr16 = getelementptr i8, ptr %filename_ptr15, i64 8
  store i8 0, ptr %elem_ptr16, align 1
  %filename_ptr17 = getelementptr [16 x i8], ptr %filename, i64 0, i64 0
  %0 = ptrtoint ptr %filename_ptr17 to i64
  %syscall_result = call i64 asm sideeffect "syscall", "={rax},{rax},{rdi},{rsi},{rdx}"(i64 2, i64 %0, i64 577, i64 438)
  %syscall_cast = trunc i64 %syscall_result to i32
  store i32 %syscall_cast, ptr %fd, align 4
  call void @puts(ptr @1)
  %1 = load i32, ptr %fd, align 4
  %2 = sext i32 %1 to i64
  %syscall_result18 = call i64 asm sideeffect "syscall", "={rax},{rax},{rdi}"(i64 3, i64 %2)
  %syscall_cast19 = trunc i64 %syscall_result18 to i32
  store i32 %syscall_cast19, ptr %status, align 4
  call void @puts(ptr @2)
  %result = alloca %result_int8, align 8
  %err_ptr = getelementptr inbounds %result_int8, ptr %result, i32 0, i32 0
  store i8 0, ptr %err_ptr, align 1
  %val_ptr = getelementptr inbounds %result_int8, ptr %result, i32 0, i32 1
  store i8 0, ptr %val_ptr, align 1
  %result_val = load %result_int8, ptr %result, align 1
  ret %result_int8 %result_val
}

define i64 @main() {
entry:
  call void @__aria_module_init()
  %0 = call %result_int8 @__user_main()
  ret i64 0
}
