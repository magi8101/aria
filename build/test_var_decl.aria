// Test variable declaration order

func:test1 = *int8() {
    int8:a = 0;
    uint8:b = 0;
    
    a = 5;
    b = 10;
    
    return 0;
};

func:test2 = *int8() {
    int8:a = 0;
    
    a = 5;
    
    uint8:b = 0;  // Is this allowed after an assignment?
    b = 10;
    
    return 0;
};

func:main = *int8() {
    test1();
    test2();
    return 0;
};
