// test_wildx_cast.aria
// Test casting wildx pointer to func

func:main = int8() {
    // Allocate executable memory
    wildx uint8:my_buffer = 0xC3;  // x86-64 RET instruction
    
    // Try to cast to func type
    func:jit_func = (func)my_buffer;
    
    print("Wildx to func cast test");
    
    *0;
};
