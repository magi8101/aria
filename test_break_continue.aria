// Test break and continue
func:main = *int8() {
    int64:sum = 0;
    
    // Test continue - skip even numbers
    till(10, 1) {
        if ($ % 2 == 0) {
            continue;
        }
        sum = sum + $;
    }
    // sum should be 1+3+5+7+9 = 25
    
    // Test break - stop at 5
    int64:count = 0;
    till(100, 1) {
        if ($ >= 5) {
            break;
        }
        count = count + 1;
    }
    // count should be 5
    
    return 0;
};
