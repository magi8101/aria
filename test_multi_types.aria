// Test parametric result types with multiple types

// int8 result
func:add8 = *int8(int8:a, int8:b) {
    return a + b;
};

// int32 result
func:add32 = *int32(int32:a, int32:b) {
    return a + b;
};

// int64 result
func:add64 = *int64(int64:a, int64:b) {
    return a + b;
};

func:main = *int8() {
    int8:r1 = add8(10, 20) ? 0;
    int32:r2 = add32(100, 200) ? 0;
    int64:r3 = add64(1000, 2000) ? 0;
    return 0;
};
