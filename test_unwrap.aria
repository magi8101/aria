// Test unwrap operator with parametric result types

func:safe_divide = int8(int8:a, int8:b) {
    return {err:NULL, val:a/b};
};

func:main = *int8() {
    // Test unwrap with success case
    int8:result1 = safe_divide(10, 2) ? -1;
    
    // Test unwrap with error case (divide by zero)
    int8:result2 = safe_divide(10, 0) ? -1;
    
    return 0;
};
