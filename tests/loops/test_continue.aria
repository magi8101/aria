// Test continue statement in till loop
func:test_continue = int32(int32:val) {
    pass(val);
};

func:main = int32() {
    till(10, 1) {
        when ($ == 5) {
            continue;  // Skip when $ is 5
        } then {
            test_continue($);
        } end {
            test_continue($);
        }
    }
    pass(0);
};
