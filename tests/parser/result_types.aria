// Result Type Parsing Tests
// Tests for research_016 functional types implementation
// Testing if parser can handle result<T, E> syntax

// Test 1: Simple result types
func:test_simple = result<int32, string>(int32:a, int32:b) {
    pass(42);
};

// Test 2: Nested result types
func:test_nested = result<result<int32, string>, string>(int32:x) {
    pass(42);
};

// Test 3: Result with exotic types
func:test_tbb = result<tbb8, string>(tbb8:a, tbb8:b) {
    pass(a);
};

// Test 4: Result with multiple parameters
func:test_multi = result<int32, int8>(int32:a, int32:b, int32:c) {
    pass(a);
};

// Test 5: Result with different error types
func:test_error_types_a = result<int32, string>(int32:x) {
    pass(x);
};

func:test_error_types_b = result<int32, int8>(int32:x) {
    pass(x);
};

func:test_error_types_c = result<string, int32>(string:s) {
    pass(s);
};
