// Test mathematical operations with return values
// NEW SYNTAX: func:name = returnType(params) { body }
{
func:add = *int8(int8:a, int8:b) {
    return a + b;
};

func:sub = *int8(int8:a, int8:b) {
    return a - b;
};

func:mul = *int8(int8:a, int8:b) {
    return a * b;
};

print("Testing math operations...");
add(10, 5);   // Should return 15
sub(10, 5);   // Should return 5
mul(10, 5);   // Should return 50
print("Math operations complete!");
}
