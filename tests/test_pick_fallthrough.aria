// Test pick with fall() explicit fallthrough
// Aria v0.0.6 Spec Section 8.3

int8:c = 5;
int8:output = 0;

pick(c) {
   (<9) {
       output = 1;
       fall(done);
   },
   (>9) {
       output = 2;
       fall(done);
   },
   (9) {
       output = 3;
       fall(done);
   },
   done:(!) {
       output = 99;
   }
}
