// Minimal pick test
func:test_pick = *int8(int8:n) {
    int8:out_val = 0;
    pick (n) {
        (0) { output = 10; },
        (1) { output = 20; },
        (*) { output = -1; }
    }
    return output;
};

func:main = *int8() {
    int8:r = test_pick(1) ? -1;
    print("Pick test complete");
    return 0;
};
