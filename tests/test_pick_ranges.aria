// Test pick with ranges and comparisons
func:classify_number = *int8(int8:x) {
    pick(x) {
        (0..5) {
            return 1;  // Low range (0 to 5 inclusive)
        }
        (6...10) {
            return 2;  // Mid range (6 to 9 exclusive of 10)
        }
        (>=10) {
            return 3;  // High (10 and above)
        }
        (<0) {
            return -1;  // Negative
        }
    }
    return 0;
};

func:test_ranges = *int8() {
    int8:r1 = classify_number(3) ? -1;   // Should be 1 (in 0..5)
    int8:r2 = classify_number(7) ? -1;   // Should be 2 (in 6...10)
    int8:r3 = classify_number(15) ? -1;  // Should be 3 (>=10)
    int8:r4 = classify_number(-5) ? -1;  // Should be -1 (<0)
    return r1 + r2 + r3 + r4;  // 1+2+3+(-1) = 5
};

func:main = *int8() {
    return test_ranges() ? -1;  // Unwrap the result
};
