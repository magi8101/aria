// Test Result type literals
// Spec requires: {err:NULL, val:x} or {err:error_code, val:NULL}

func:test_success = int8() {
    // Explicit success result
    return {err:0, val:42};
};

func:test_error = int8() {
    // Explicit error result  
    return {err:1, val:0};
};

func:test_partial = int8() {
    // Only val specified (err defaults to 0)
    return {val:99};
};
